{
  "name": "recipie",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "fmt": "prettier --check . && gofmt -l ./server/*.go",
    "fmt:fix": "prettier --write . && gofmt -l -w ./server/*.go",
    "build:server": "bun run docker:build",
    "server": "bun run docker:stop; bun run docker:start",
    "dev:client": "vite -c ./vite.config.ts ./client/",
    "build:client": "tsc && vite build",
    "preview": "vite preview",
    "gittree": "git log --oneline --graph --decorate --all",
    "docker:build": "docker image rm -f recipie; docker build -t recipie . --progress=plain",
    "docker:start": "docker container rm recipie; docker run -p 8090:8090 --name recipie recipie",
    "docker:stop": "docker stop recipie"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "solid-js": "^1.8.7",
    "temporal-polyfill": "^0.2.0"
  },
  "devDependencies": {
    "@solidjs/router": "^0.10.5",
    "autoprefixer": "^10.4.16",
    "husky": "^8.0.0",
    "husky-init": "^8.0.0",
    "pocketbase": "^0.20.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-solid": "^2.8.0"
  }
}